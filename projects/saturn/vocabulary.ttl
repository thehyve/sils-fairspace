    sh:name "Object" ;
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    fs:partOfHierarchy true ;
    fs:hierarchyDescendants ( sils:Sample ) ;
    sh:property
    [
        sh:name "Label" ;
        sh:description "Unique object label." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        dash:singleLine true ;
        fs:importantProperty true ;
